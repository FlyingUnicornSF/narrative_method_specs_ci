{
  "name" : "View Growth Parameters Plot",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : "kbaseGrowthParamsPlotInput",
    "output" : "kbaseGrowthParamsPlot"
  },
  "parameters" : [ 
      {
        "id" : "input_growth_matrix",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {
          "valid_ws_types" : [ "KBaseEnigmaMetals.GrowthMatrix" ]
        }
      },
      {
        "id" : "input_growth_param",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "maxRate" ],
        "field_type" : "dropdown",
        "dropdown_options":{
            "options": [
                {
                    "value": "maxRate",
                    "display": "Max growth rate"
                },
                {
                    "value": "maxRateTime",
                    "display": "Max growth rate time"
                },                
                {
                    "value": "maxOD",
                    "display": "Max OD"
                },
                {
                    "value": "maxODTime",
                    "display": "Max OD time"
                }
            ]
        }
      },
      {
        "id" : "input_condition_param",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "dropdown",
        "dropdown_options":{
            "options": [
            ]
        }
      }   
  ],
  "behavior" : {
    "none" : {
      "output_mapping" : [
        {
          "input_parameter": "input_growth_matrix",
          "target_property": "growthMatrixID"
        },
        {
          "input_parameter": "input_growth_param",
          "target_property": "growthParam"            
        },          
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspaceID"
        }
      ]
    }
  }
}
